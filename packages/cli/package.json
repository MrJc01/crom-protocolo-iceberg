{
  "name": "@iceberg/cli",
  "version": "0.1.0",
  "description": "CLI para o Protocolo Iceberg - Plataforma descentralizada de informação cidadã",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "iceberg": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "bip39": "^3.1.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "iceberg",
    "cli",
    "p2p",
    "decentralized"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrJc01/crom-protocolo-iceberg.git",
    "directory": "packages/cli"
  }
}
